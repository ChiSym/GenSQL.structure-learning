{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
        "values": "<DVC_METRIC_DATA>"
    },
    "title": "<DVC_METRIC_TITLE>",
    "width": 300,
    "height": 300,
    "mark": {
        "type": "rect",
        "tooltip": {
        "content": "data"
        }
    },
    "encoding": {
      "y": {"field": "<DVC_METRIC_Y>", "type": "nominal", "sort": <sorted_cols>, "title": "<DVC_METRIC_Y_LABEL>"},
      "x": {"field": "<DVC_METRIC_X>", "type": "nominal", "sort": <sorted_cols>, "title": "<DVC_METRIC_X_LABEL>"},
      "color": {"field": "dependence_probability", "type": "quantitative"}
    },
    "config": {
      "axis": {"grid": true, "tickBand": "extent"}
    }
  }
  